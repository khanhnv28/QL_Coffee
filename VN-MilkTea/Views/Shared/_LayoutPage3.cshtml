
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="">
    <title>Home | MilkTea-Magus</title>
    <link href="~/Assets/css/bootstrap.min.css" rel="stylesheet">
    <link href="~/Assets/css/bootstrap.css" rel="stylesheet">
    <link href="~/Assets/css/font-awesome.min.css" rel="stylesheet">
    <link href="~/Assets/css/prettyPhoto.css" rel="stylesheet">
    <link href="~/Assets/css/price-range.css" rel="stylesheet">
    <link href="~/Assets/css/animate.css" rel="stylesheet">
    <link href="~/Assets/css/main.css" rel="stylesheet">
    <link href="~/Assets/css/Style.css" rel="stylesheet">
    <link href="~/Assets/css/responsive.css" rel="stylesheet">
    <link rel="shortcut icon" href="~/Assets/images/ico/favicon.ico">
    <link rel="apple-touch-icon-precomposed" sizes="144x144" href="~/Assets/images/ico/apple-touch-icon-144-precomposed.png">
    <link rel="apple-touch-icon-precomposed" sizes="114x114" href="~/Assets/images/ico/apple-touch-icon-114-precomposed.png">
    <link rel="apple-touch-icon-precomposed" sizes="72x72" href="~/Assets/images/ico/apple-touch-icon-72-precomposed.png">
    <link rel="apple-touch-icon-precomposed" href="~/Assets/images/ico/apple-touch-icon-57-precomposed.png">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.8.0/font/bootstrap-icons.css">
    <script src="~/Assets/js/jquery.js"></script>
    <script src="~/Assets/js/bootstrap.min.js"></script>
    <script src="~/Assets/js/bootstrap.js"></script>
    <link href="~/Assets/css/custom.css" rel="stylesheet">
    <link href="~/Assets/css/custom1.css" rel="stylesheet">
    <script src="~/Assets/js/price-range.js"></script>
    <script src="~/Assets/js/jquery.prettyPhoto.js"></script>
    <script src="~/Assets/js/main.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.7.1/jquery.min.js"></script>
    <title>Ajax Search</title>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.7.1/jquery.min.js"></script>
    <script src="https://code.jquery.com/jquery-3.6.4.min.js"></script>
    
</head>
<body>
    <header id="header">
        <div class="header_top">
            <div class="container">
                <div class="row">
                    <div class="col-sm-6">
                        <div class="contactinfo">
                            <ul class="nav nav-pills">
                                <li><a href="#"><i class="fa fa-phone"></i> 0818594119</a></li>
                                <li><a href="#"><i class="fa fa-envelope"></i> ngokhanh280303@gmail.com</a></li>
                            </ul>
                        </div>
                    </div>
                    <div class="col-sm-6">
                        <div class="social-icons pull-right">
                            <ul class="nav navbar-nav">
                                <li><a href="https://www.facebook.com/magus283"><i class="fa fa-facebook"></i></a></li>
                                <li><a href="#"><i class="fa fa-twitter"></i></a></li>
                                <li><a href="#"><i class="fa fa-linkedin"></i></a></li>
                                <li><a href="#"><i class="fa fa-dribbble"></i></a></li>
                                <li><a href="#"><i class="fa fa-google-plus"></i></a></li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="header-middle">
            <!--header-middle-->
            <div class="container">
                <div class="row">
                    <div class="col-sm-4">
                        <div class="logo pull-left">
                            <a href="~/home/index"><img src="~/Assets/images/home/logo.png" alt="" /></a>
                        </div>
                    </div>
                    <div class="col-sm-8">
                        <div class="shop-menu pull-right ">
                            <ul class="nav navbar-nav">
                                @if (User.Identity.IsAuthenticated == true)
                                {
                                    <li><a href="#" style="background: #212529;"><i class="fa fa-user"></i>Welcome @User.Identity.Name</a></li>
                                }
                                @if (User.Identity.IsAuthenticated == false)
                                {
                                    <li><a href="#" style="background: #212529;"><i class="fa fa-user"></i>Tài khoản</a></li>
                                }
                                <li><a href="/cart/wishlist" style="background: #212529;" id="wishlist-button"><i class="fa fa-star"></i> Danh sách yêu thích</a></li>
                                <li><a href="/cart/index" style="background: #212529;"><i class="fa fa-shopping-cart"><span class="badge badge-danger navbar-badge" id="CartCount">@Session["count"]</span></i> Giỏ hàng</a></li>
                                @if (User.Identity.IsAuthenticated == false)
                                {
                                    <li><a href="/Account/login" style="background: #212529;"><i class="fa fa-lock"></i> Đăng nhập</a></li>
                                    <li><a href="/Account/register" style="background: #212529;"><i class="fa fa-lock"></i> Đăng ký</a></li>
                                }
                                @if (User.Identity.IsAuthenticated == true)
                                {
                                    <li><a href="/Account/logout" style="background: #212529;">Đăng xuất</a></li>
                                }
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="header-bottom">
            <!--header-bottom-->
            <div class="container">
                <div class="row">
                    <div class="col-sm-9">
                        <div class="navbar-header">
                            <button type="button" class="navbar-toggle" data-toggle="collapse"
                                    data-target=".navbar-collapse">
                                <span class="sr-only">Toggle navigation</span>
                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>
                            </button>
                        </div>
                        <div class="mainmenu pull-left">
                            <ul class="nav navbar-nav collapse navbar-collapse">
                                <li><a href="/home/index" class="active">Trang chủ</a></li>
                                <li class="dropdown">
                                    <a href="/home/getproductajax">Cửa hàng<i class="fa fa-angle-down"></i></a>
                                    <ul role="menu" class="sub-menu">
                                        <li><a href="/home/showproduct">Sản phẩm</a></li>
                                        <li><a href="/cart/index">Giỏ hàng</a></li>
                                        @if (User.Identity.IsAuthenticated == false)
                                        {
                                            <li><a href="/account/login">Đăng nhập</a></li>
                                        }
                                    </ul>
                                </li>
                                <li><a href="/home/contact">Liên hệ</a></li>
                            </ul>
                        </div>
                    </div>
                    @RenderSection("HomeSearch", required: false)
                </div>
            </div>
            <!--/header-bottom-->
        </div>
    </header>
    @RenderSection("Cate_box", required: false)
    <section>
        <div class="container">
            <div class="row">
                @RenderSection("Sidebar", required: false)
                <div>
                    @RenderBody()
                    @RenderSection("Product", required: false)
                    @RenderSection("GetproductAjax", required: false)
                    @RenderSection("Details", required: false)
                </div>
            </div>
            
        </div>
    </section>
    <footer id="footer">
        <div class="footer-top">
            <div class="container">
                <div class="row">
                    <div class="col-sm-3">
                        <div class="companyinfo">
                            <h2><span>MilkTea</span>-Magus</h2>
                        </div>
                    </div>
                    <div class="col-sm-6">
                        <div class="address">
                            <img src="~/Assets/images/home/map.png" alt="" />
                            <p>505 S Atlantic Ave Virginia Beach, VA(Virginia)</p>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </footer>

</body>
</html>
